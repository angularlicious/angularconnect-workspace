{
  "name": "workspace",
  "version": "0.0.2",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged  --pattern=\"**/*.*(ts|json)\" --verbose"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check-DOES-NOTHING": "./node_modules/.bin/nx format:check-all",
    "format:check": "yarn format:check-apps",
    "format:check-apps": "prettier --config ./.prettierrc --list-different \"apps/**/src/{app,environments,assets}/**/*{.ts,.json}\"",
    "format:check-libs": "prettier --config ./.prettierrc --list-different \"libs/**/src/**/*{.ts,.js,.json}\"",
    "format:fix-apps": "prettier --config ./.prettierrc --write --list-different \"apps/**/src/{app,environments,assets}/**/*{.ts,.json}\"",
    "format:fix-libs": "prettier --config ./.prettierrc --write --list-different \"libs/**/src/**/*{.ts,.js,.json}\"",
    "package:libs": "yarn run package:configuration && yarn run package:rules-engine && yarn run package:actions && yarn run package:logging  && yarn run package:foundation && yarn run package:error-handling && yarn run package:http-service && yarn run package:components",
    "package:configuration": "ng build configuration && npm link dist/libs/configuration",
    "package:rules-engine": "ng build rules-engine && npm link dist/libs/rules-engine",
    "package:actions": "ng build actions && npm link dist/libs/actions",
    "package:logging": "ng build logging && npm link dist/libs/logging",
    "package:error-handling": "ng build error-handling && npm link dist/libs/error-handling",
    "package:foundation": "ng build foundation && npm link dist/libs/foundation",
    "package:http-service": "ng build http-service && npm link dist/libs/http-service",
    "package:components": "ng build components && npm link dist/libs/components",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": true,
  "dependencies": {
    "ajv": "6.10.0",
    "express": "4.17.1",
    "@agm/core": "1.0.0-beta.6",
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "~8.1.4",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "^8.0.0",
    "@angular/material": "^8.1.4",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@nrwl/angular": "^8.2.0",
    "arrive": "2.4.1",
    "bootstrap-material-design": "4.1.2",
    "bootstrap-notify": "3.1.3",
    "bootstrap": "4.3.1",
    "chart.js": "2.8.0",
    "chartist": "0.11.2",
    "core-js": "^2.5.4",
    "firebase": "^6.6.0",
    "global": "^4.4.0",
    "googleapis": "40.0.0",
    "hammerjs": "2.0.8",
    "jquery": "3.5.0",
    "moment": "2.24.0",
    "ngx-loggly-logger": "7.0.3",
    "perfect-scrollbar": "1.1.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "typescript-dotnet-commonjs": "4.11.2",
    "web-animations-js": "2.3.1",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "jest-preset-angular": "7.0.0",
    "@angular-devkit/build-angular": "^0.800.0",
    "@angular-devkit/build-ng-packagr": "~0.800.0",
    "@angular/cli": "8.0.0",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@nrwl/cypress": "8.2.0",
    "@nrwl/jest": "8.2.0",
    "@nrwl/workspace": "8.2.0",
    "@types/bootstrap": "4.3.0",
    "@types/chartist": "0.9.46",
    "@types/googlemaps": "3.36.4",
    "@types/jasmine": "3.3.13",
    "@types/jest": "24.0.9",
    "@types/jquery": "3.3.29",
    "@types/node": "12.0.7",
    "codelyzer": "5.1.0",
    "cypress": "~3.3.1",
    "dotenv": "6.2.0",
    "firebase-tools": "^7.1.0",
    "husky": "^3.0.0",
    "jest": "24.1.0",
    "ng-packagr": "^5.1.0",
    "node-jq": "^1.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.11.1",
    "ts-jest": "24.0.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5"
  }
}
